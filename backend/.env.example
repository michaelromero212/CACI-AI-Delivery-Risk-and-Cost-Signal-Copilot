# Environment Configuration
# Copy this file to .env and fill in the values

# Hugging Face API Key (optional - fallback mode works without it)
HUGGINGFACE_API_KEY=

# Database URL (defaults to SQLite if not set)
# DATABASE_URL=postgresql://user:password@localhost:5432/copilot

# Model to use (default: mistralai/Mistral-7B-Instruct-v0.2)
# HUGGINGFACE_MODEL=mistralai/Mistral-7B-Instruct-v0.2

# Debug mode
DEBUG=true
